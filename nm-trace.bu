variant: fcos
version: 1.1.0
passwd:
  users:
    - name: core
      password_hash: ...
      ssh_authorized_keys:
        - ssh-rsa AAAA...
storage:
  files:
    -
      path: /etc/NetworkManager/conf.d/10-log-level-trace.conf
      mode: 0644
      contents:
        inline: |
          [logging]
          level=trace
